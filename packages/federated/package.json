{
  "name": "@cloudmosaic/quickstarts-federated",
  "private": true,
  "version": "0.0.1",
  "description": "Quickstarts webpack app for MAS",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/module/index.es.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cloudmosaic/quickstarts.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cloudmosaic/quickstarts/issues"
  },
  "homepage": "https://github.com/cloudmosaic/quickstarts#readme",
  "federatedModuleName": "federatedQuickstarts",
  "crc": {
    "name": "rhosak-quickstarts",
    "bundle": "application-services",
    "beta": true
  },
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "yarn clean",
    "dr:surge": "node dr-surge.js",
    "build": "yarn build:webpack",
    "build:module": "yarn clean && webpack --config webpack.module.prod.js",
    "build:ts": "yarn clean && tsc --p tsconfig.module.json",
    "build:webpack": "yarn clean && webpack --config webpack.app.prod.js && yarn dr:surge",
    "start": "node server.js",
    "start:dev": "webpack serve --hot --color --progress --config webpack.app.dev.js",
    "start:federate": "HOST=localhost PORT=9000 PROTOCOL=http webpack serve --hot --color --progress --config webpack.app.dev.js",
    "test": "jest",
    "eslint": "eslint --ext .tsx,.js ./src/",
    "lint": "yarn eslint",
    "format": "prettier --check --write ./src/**/*.{tsx,ts}",
    "type-check": "tsc --noEmit",
    "ci-checks": "yarn type-check && yarn lint && yarn test",
    "build:bundle-profile": "webpack --config webpack.module.prod.js --profile --json > stats.json",
    "bundle-profile:analyze": "yarn build:bundle-profile && webpack-bundle-analyzer ./stats.json"
  },
  "peerDependencies": {
    "react": ">=16.14.0",
    "react-dom": ">=16.14.0",
    "react-router-dom": ">=5.2.0"
  },
  "dependencies": {
    "@patternfly/react-catalog-view-extension": "^4.8.126",
    "@patternfly/react-core": "^4.84.4",
    "@patternfly/react-table": "^4.19.45",
    "@redhat-cloud-services/frontend-components": "^2.4.22",
    "dayjs": "^1.9.6",
    "i18next": "^19.8.3",
    "i18next-browser-languagedetector": "^6.0.1",
    "i18next-http-backend": "^1.0.21",
    "i18next-parser": "^3.3.0",
    "i18next-pseudo": "^2.2.0",
    "immutable": "^4.0.0-rc.12",
    "lodash-es": "^4.17.20",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.7.3",
    "react-redux": "^7.2.2",
    "react-router-last-location": "^2.0.1",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^6.1.2",
    "sass": "^1.30.0",
    "showdown": "^1.9.1",
    "typesafe-actions": "^5.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.1.0",
    "@testing-library/react": "^11.2.2",
    "@types/enzyme": "^3.10.7",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/history": "^4.7.8",
    "@types/jest": "^26.0.14",
    "@types/react-router-dom": "^5.1.6",
    "@types/victory": "^33.1.4",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "copy-webpack-plugin": "^6.2.1",
    "css-loader": "^5.0.0",
    "dotenv-webpack": "^4.0.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.11.0",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "express": "^4.17.1",
    "file-loader": "^6.1.1",
    "html-webpack-plugin": "^5.0.0-beta.6",
    "imagemin": "^7.0.0",
    "jest": "^26.5.3",
    "mini-css-extract-plugin": "^1.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "prettier": "^2.1.2",
    "prop-types": "^15.6.1",
    "raw-loader": "^4.0.2",
    "react": "^16.14.0",
    "react-axe": "^3.5.4",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^16.14.0",
    "react-router-dom": "^5.2.0",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "^3.0.2",
    "rollup": "^2.37.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-scss": "^2.6.1",
    "rollup-plugin-typescript2": "^0.29.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "svg-url-loader": "^6.0.0",
    "terser-webpack-plugin": "^5.0.0",
    "ts-jest": "^26.4.1",
    "ts-loader": "^8.0.5",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "tslib": "^2.0.3",
    "typescript": "^4.0.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.1.3",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^4.0.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.2.0",
    "webpack-node-externals": "^2.5.2"
  }
}
